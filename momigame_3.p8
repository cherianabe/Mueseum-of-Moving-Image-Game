pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
--main

simple_mode = true

function _init()
	
	player= {
		x = 59,
		y = 59,
		w = 8,
		h = 8,
		flp = false,
		leftright = 0,
		dy = 0,
		max_dx = 2,
		max_dy = 3,
		acc = 0.5,
		boost = 4,
		anim = 0,
		running = false,
		jumping = false, 
		falling = false,
		sliding = false, 
		landed = false
	}
	
	timing = 0.25
	gravity = 0.3
	friction = 0.85
	cam_x = 0
	
	map_start = 0
	map_end = 1024
	
	palt(0,false)
	palt(2,true)
	
end

function _update()
	
	cam_x = player.x - 64 + (player.w/2)
	if cam_x < map_start then
	cam_x = map_start
	end
	if cam_x > map_end - 128
	then
	cam_map = map_end-128
	end
	camera(cam_x,0)
	
	player_update()
	
end

function _draw()
	
	cls(0)
	map(0,0)
	spr(8, player.x, player.y, 2,2)
	
	palt(0,false)
	
	palt(2,true)
	spr(12,689,80,2,2)
	spr(34,650,96,2,2)
	spr(192, 665,96, 2,2)
	spr(224, 700,96, 2,2)
	
end
-->8
--collisions

--function for collisions

function collide_map(obj,drc,flag)
	--obj = table needs x,y,w,h
	--aim = left, right, up, down
	local x = obj.x local y = obj.y
	local w = obj.w local h = obj.h
	
	local x1 = 0
	local y1 = 0
	local x2 = 0
	local y2 = 0
	
	if drc == "left" then
	x1 = x-1 y1 = y
	x2 = x   y2 = y+h-1
	
	elseif drc == "right" then
	x1 = x+w   y1 = y
	x2 = x+w+1 y2 = y+h-1
	
	elseif drc == "up" then
	x1 = x+1   y1 = y-1
	x2 = x+w-1 y2 = y
	
	elseif drc == "down" then
	x1 = x     y1 = y+h
	x2 = x+w   y2 = y+h
	end
	
	--pixels to tiles
	x1/=8   y1/=8
	x2/=8   y2/=8
	
	if fget(mget(x1,y1),flag)
	or fget(mget(x1,y2),flag)
	or fget(mget(x2,y1),flag)
	or fget(mget(x2,y2),flag)
	
	then 
	
	return
	true
	
	else
	return 
	false
	end
	
end
-->8
function player_update()
	
	player.dy += gravity
	player.leftright *= friction
	-- adjust controls here!
	
	if simple_mode then
	
	if btn(0) then //left arrow
			
		player.leftright -= 0
			
	end
		
	if btn(1) then //right arrow
			
		player.leftright += 0
			
	end
		
	end
	-- help miss piggy reach kermit!
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	-- collision down
	if player.dy > 0 then
		player.falling = true
		player.landed = false
		player.jumping = false
		if collide_map(player,"down",0) then
			player.landed = true
			player.falling = false
			player.dy = 0
			player.y -= (player.y+player.h)%8
		end
	elseif player.dy < 0 then
		player.jumping = true
		
	end
	-- collision left/right
	if player.leftright < 0 then
		if collide_map(player,"left",1) then
			player.leftright = 0
		end
	elseif player.leftright > 0 then
		if collide_map(player,"right",1) then
			player.leftright = 0
		end
	end
	-- apply movement
	player.x += player.leftright
	player.y += player.dy
	
end

